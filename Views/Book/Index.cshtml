@model BookListViewModel
@{
    ViewData["Title"] = "Books";
}

 <div class="row">
     <div class="col-md-12">
         <ul>
             @foreach (var book in Model.Books)
             {
                 <li><a href="./@book.id">@book.Title, @book.Author, @book.ISBN, @book.CreatedOn.ToString("d")</a></li>
             }
         </ul>
     </div>
 </div>

